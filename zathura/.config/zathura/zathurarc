# Enable links
set sandbox none
# Set clipboard
set selection-clipboard clipboard
# Set custom key mappings
map <C-j> zoom out
map <C-k> zoom in
# Layout
set page-padding 4
# Colors
set highlight-color \#e5c07b
set highlight-active-color \#61afef
set recolor false
set recolor-lightcolor \#313640
set recolor-darkcolor \#dcdfe4
set recolor-keephue
set default-bg \#282c34
